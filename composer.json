{
    "name": "jaythegeek/laravelscaffolding",
    "description": "Scaffold out passwordless laravel setup",
    "license": "license",
    "authors": [
        {
            "name": "Alex Jay",
            "email": "alex@jaythegeek.io",
            "homepage": "https://jaythegeek.io"
        }
    ],
    "homepage": "https://github.com/jaythegeek/laravelscaffolding",
    "keywords": [
        "Laravel",
        "LaravelScaffolding"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JayTheGeek\\LaravelScaffolding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JayTheGeek\\LaravelScaffolding\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JayTheGeek\\LaravelScaffolding\\LaravelScaffoldingServiceProvider"
            ],
            "aliases": {
                "LaravelScaffolding": "JayTheGeek\\LaravelScaffolding\\Facades\\LaravelScaffolding"
            }
        }
    }
}