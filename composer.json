{
  "name": "jaythegeek/laravel-scaffold",
  "description": "Scaffolding for passwordless laravel setup, vue navigation and a maker command for generating a model, migration, controller, routes and factory from the command line.",
  "keywords": ["laravel", "generators", "scaffolding"],
  "license": "MIT",
  "authors": [{
    "name": "Alex Jay",
    "email": "alex@jaythegeek.io"
  }],
  "require": {
    "php": "^7.2.5",
    "illuminate/support": "7.6.*",
    "illuminate/console": "7.6.*",
    "illuminate/filesystem": "7.6.*"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelScaffold\\ResourceGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelScaffold\\ResourceGenerator\\": ["src/", "tests/"],
      "App\\": "tests/generated/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JayTheGeek\\LaravelScaffold\\Commands\\ResourceMakeCommand"
      ],
      "aliases": {
        "LaravelScaffold": "JayTheGeek\\LaravelScaffold\\Facade"
      }
    }
  }
}