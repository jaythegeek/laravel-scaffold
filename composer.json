{
    "name": "jaythegeek/laravel-scaffold",
    "description": "Scaffold out passwordless laravel setup",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Jay",
            "email": "alex@jaythegeek.io",
            "homepage": "https://jaythegeek.io"
        }
    ],
    "homepage": "https://github.com/jaythegeek/laravel-scaffold",
    "keywords": [
        "Laravel",
        "LaravelScaffold"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5"
    },
    "autoload": {
        "psr-4": {
            "JayTheGeek\\LaravelScaffold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JayTheGeek\\LaravelScaffold\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JayTheGeek\\LaravelScaffold\\LaravelScaffoldServiceProvider"
            ],
            "aliases": {
                "LaravelScaffold": "JayTheGeek\\LaravelScaffold\\Facades\\LaravelScaffold"
            }
        }
    }
}
